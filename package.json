{
    "name": "redscript-syntax-highlighting",
    "displayName": "redscript-syntax-highlighting",
    "description": "Syntax Highlighting for Redscript",
    "repository": "https://github.com/jackhumbert/redscript-syntax-highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "redscript",
            "aliases": ["Redscript", "redscript"],
            "extensions": [".reds"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "redscript",
            "scopeName": "source.redscript",
            "path": "./syntaxes/redscript.tmLanguage.json"
        }]
    }
}